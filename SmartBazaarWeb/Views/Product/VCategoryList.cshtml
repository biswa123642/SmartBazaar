@model List<SmartBazaar.Web.Models.Site.MenuViewModel>

<div class="list-group">
    @foreach (var item in Model)
    {
        <a class="list-group-item" href="@Url.Action("Index", "Product", new { id = item.Id.ToString() + "-" + item.Title , q = Request.QueryString["q"] })">@item.Title</a>
    }
</div>